programa {
  funcao inicio() {
    inteiro numero1 = 100 , numero2 = 5
    /* operadores de atribuicao
    ++  variavel = variavel +1
    --  variavel = variavel -1
    += variavel = variavel + outro valor
    -= variavel = variavel - outro valor
    *= variavel = variavel * outro valor
    /= variavel = variavel / outro valor
    */
    numero1 += numero2
  }
}
