programa {
  funcao inicio() {
    caracter resposta_alladin , resposta_jasmin
    escreva(" alladin foi ao encontro? (s / n)")
    leia(resposta_alladin) 
    escreva(" jasmin foi ao encontro? (s / n)")
    leia(resposta_jasmin) 
    se (resposta_alladin == 's') e (resposta_jasmin == 's'){
       
       escreva(" ouve um encontro")
    }
    senao {
   escreva(" nao ouve encontro ")   
    }


  }
}
